creates deployment if dev_mode is enabled:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      labels:
        app.kubernetes.io/component: oncall-ui
      name: oncall-ui
    spec:
      replicas: 1
      selector:
        matchLabels:
          app.kubernetes.io/component: oncall-ui
      template:
        metadata:
          labels:
            app.kubernetes.io/component: oncall-ui
        spec:
          containers:
            - env: null
              image: oncall/ui:dev
              imagePullPolicy: IfNotPresent
              name: oncall-ui
              resources:
                limits:
                  cpu: "1.5"
                requests:
                  cpu: "1.0"
              volumeMounts:
                - mountPath: /etc/app
                  name: hot-reloaded-plugin
          volumes:
            - hostPath:
                path: /oncall-plugin
              name: hot-reloaded-plugin
